<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Options"></activity>
        <activity android:name=".singlePlayer"
            android:parentActivityName=".name1"
            ></activity>
        <activity android:name=".multiPlayer"
            android:parentActivityName=".name2"
            ></activity>
        <activity android:name=".about"
            android:parentActivityName=".Options"
            ></activity>
        <activity android:name=".name1"
            android:parentActivityName=".Options"
            ></activity>
        <activity android:name=".name2"
            android:parentActivityName=".Options"
            ></activity>
        <activity android:name=".rules"
            android:parentActivityName=".Options"
            ></activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>